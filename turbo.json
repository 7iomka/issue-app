{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {},
    "start": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "dev-clean": {
      "cache": false
    },
    "dev-shapes": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "build-shapes": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "api": {
      "cache": false
    },
    "storybook": {
      "cache": false
    },
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    }
  }
}
